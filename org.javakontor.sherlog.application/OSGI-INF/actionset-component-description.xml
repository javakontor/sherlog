<?xml version="1.0"?>
<component name="actionGroupRegistryComponent" immediate="true">
  <implementation class="org.javakontor.sherlog.application.action.impl.ActionSetComponent"/>
  	<service>
		<provide
			interface="org.javakontor.sherlog.application.action.ActionSetManager" />
	</service>
  
  <reference name="actions"
    interface="org.javakontor.sherlog.application.action.Action"
    bind="addAction"
    unbind="removeAction"
    cardinality="0..n"
    policy="dynamic"
  />
  <reference name="actionGroups"
    interface="org.javakontor.sherlog.application.action.ActionGroup"
    bind="addActionGroup"
    unbind="removeActionGroup"
    cardinality="0..n"
    policy="dynamic"
  />
