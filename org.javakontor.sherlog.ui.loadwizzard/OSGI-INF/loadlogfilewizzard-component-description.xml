<?xml version="1.0"?>
<component name="loadLogFileWizzardComponent" immediate="true">
	<implementation
		class="org.javakontor.sherlog.ui.loadwizzard.internal.LoadLogFileWizzardComponent" />
	<reference name="logEventStore"
		interface="org.javakontor.sherlog.core.store.LogEventStore"
		bind="setLogEventStore" unbind="unsetLogEventStore"
		cardinality="1..1" policy="dynamic" />
	<reference name="logEventReaderFactory"
		interface="org.javakontor.sherlog.core.reader.LogEventReaderFactory"
		bind="setLogEventReaderFactory" unbind="unsetLogEventReaderFactory"
		cardinality="0..1" policy="dynamic" />
</component>