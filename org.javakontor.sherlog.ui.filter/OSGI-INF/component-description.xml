<?xml version="1.0"?>
<component name="filterConfigurationEditorFactoryManagerComponent"
	immediate="true">
	<implementation
		class="org.javakontor.sherlog.ui.filter.internal.FilterConfigurationEditorFactoryManagerComponent" />

	<service>
		<provide
			interface="org.javakontor.sherlog.ui.filter.FilterConfigurationEditorFactoryManager" />
	</service>

	<reference name="filterConfigurationEditorFactory"
		interface="org.javakontor.sherlog.ui.filter.FilterConfigurationEditorFactory"
		bind="addFilterConfigurationEditorFactory"
		unbind="removeFilterConfigurationEditorFactory" cardinality="0..n"
		policy="dynamic" />

</component>